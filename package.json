{
  "name": "chat_app",
  "version": "1.0.0",
  "description": "Chat application for learning( Vue and Node js )",
  "main": "index.js",
  "scripts": {
    "postinstall": "NODE_ENV=development && npm run client-install && NODE_ENV=production && npm run client-build",
    "dev": "nodemon server/index.js",
    "client": "npm run dev --prefix client",
    "client-install": "npm install --prefix client",
    "client-build": "npm run build --prefix client",
    "start": "node server/index.js"
  },
  "author": "Narenderen",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-static-gzip": "^2.0.6",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.5",
    "mongoose": "^5.9.7",
    "socket.io": "^2.3.0"
  }
}
